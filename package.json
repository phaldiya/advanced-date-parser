{
  "name": "advanced-date-parser",
  "description": "A date parser middleware, to parse date into Javascript Date Objects",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "authors": [
    "Pradeep K Haldiya <pradeep.haldiya@gmail.com>"
  ],
  "keywords": [
    "express",
    "expressjs",
    "date",
    "datetime",
    "middleware",
    "ts",
    "typescript",
    "types",
    "typing",
    "typings",
    "body",
    "query",
    "pradeep",
    "haldiya"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaldiya/advanced-date-parser.git"
  },
  "bugs": {
    "url": "https://github.com/phaldiya/advanced-date-parser/issues"
  },
  "homepage": "https://github.com/phaldiya/advanced-date-parser#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^25.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  }
}
